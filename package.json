{
  "name": "@madoc/component-repl",
  "description": "Madoc REPL Component",
  "main": "./index.js",
  "version": "0.0.6",
  "author": "Jérémie Pichon <contact@cadgerfeast.com>",
  "license": "MIT",
  "keywords": [
    "madoc",
    "component",
    "repl",
    "madoc-component-repl"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cadgerfeast/madoc-component-repl.git"
  },
  "files": [
    "assets/**/*",
    "dist/Repl.common.js",
    "index.js"
  ],
  "scripts": {
    "dev": "npm run build:lib && run-p dev:lib dev:docs",
    "dev:lib": "vue-cli-service build --watch --target lib --name Repl src/index.js",
    "dev:docs": "madoc dev",
    "lint": "vue-cli-service lint",
    "test": "vue-cli-service test:unit",
    "build": "run-s build:lib build:docs",
    "build:lib": "vue-cli-service build --target lib --name Repl src/index.js",
    "build:docs": "madoc build",
    "serve": "madoc serve"
  },
  "dependencies": {
    "@madoc/core": "0.0.14",
    "@vue/cli-plugin-babel": "4.5.0",
    "@vue/cli-service": "4.5.0",
    "core-js": "3.6.5",
    "monaco-editor": "0.20.0",
    "node-sass": "4.12.0",
    "sass-loader": "8.0.2",
    "vue": "2.6.11",
    "vue-template-compiler": "2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-eslint": "4.5.0",
    "@vue/cli-plugin-unit-jest": "4.5.0",
    "@vue/test-utils": "1.0.3",
    "babel-eslint": "10.1.0",
    "eslint": "6.7.2",
    "eslint-plugin-vue": "6.2.2",
    "monaco-editor-webpack-plugin": "1.9.1",
    "npm-run-all": "4.1.5"
  }
}
